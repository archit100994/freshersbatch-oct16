package Spring.SpringCore;

public class Order
{
	String name;
	int price;
	
	public Order()
	{
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "Order [name=" + name + ", price=" + price + "]";
	}
	
	
}
