package Spring.SpringCore;

public class Order
{
	int id;
	Item item;
	
	public Order()
	{
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Item getItem() {
		return item;
	}

	public void setItem(Item item) {
		this.item = item;
	}

	@Override
	public String toString() {
		return "Order [name=" + name + ", item=" + item + "]";
	}

}
